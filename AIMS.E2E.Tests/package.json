{
  "name": "aims-e2e-tests",
  "version": "1.0.0",
  "private": true,
  "description": "Playwright end-to-end tests for AIMS web app",
  "scripts": {
    "install-deps": "npx playwright install --with-deps",
    "test:e2e": "playwright test --config=playwright.config.ts",
    "test:headed": "playwright test --config=playwright.config.ts --headed",
    "test:trace": "playwright test --config=playwright.config.ts --trace on",
    "test:local": "cross-env BASE_URL=http://localhost:5119 USE_TEST_AUTH=true playwright test --config=playwright.config.ts"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@types/node": "^24.9.1",
    "cross-env": "^7.0.3",
    "typescript": "^5.9.3"
  },
  "main": "index.js",
  "dependencies": {
    "cross-spawn": "^7.0.6",
    "fsevents": "^2.3.2",
    "isexe": "^2.0.0",
    "path-key": "^3.1.1",
    "playwright": "^1.56.1",
    "playwright-core": "^1.56.1",
    "shebang-command": "^2.0.0",
    "shebang-regex": "^3.0.0",
    "which": "^2.0.2"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}

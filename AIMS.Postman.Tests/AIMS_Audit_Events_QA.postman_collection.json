			"listen": "prerequest",
			"script": {
				"exec": [
					"// Only fetch if we don't already have one or it's older than ~55 minutes",
					"function expired(ts) { return !ts || (Date.now() - Number(ts)) > (55*60*1000); }",
					"",
					"if (!pm.collectionVariables.get('access_token') || expired(pm.collectionVariables.get('access_token_ts'))) {",
					"  const tenant = pm.variables.get('tenantId');",
					"  const clientId = pm.variables.get('clientId');",
					"  const clientSecret = pm.variables.get('clientSecret');",
					"  const scope = pm.variables.get('apiScope');",
					"",
					"  if (!tenant || !clientId || !clientSecret || !scope) {",
					"    console.warn('[auth] Missing tenantId/clientId/clientSecret/apiScope');",
					"  } else {",
					"    pm.sendRequest({",
					"      url: `https://login.microsoftonline.com/${tenant}/oauth2/v2.0/token`,",
					"      method: 'POST',",
					"      header: { 'Content-Type': 'application/x-www-form-urlencoded' },",
					"      body: {",
					"        mode: 'urlencoded',",
					"        urlencoded: [",
					"          { key: 'grant_type', value: 'client_credentials' },",
					"          { key: 'client_id', value: clientId },",
					"          { key: 'client_secret', value: clientSecret },",
					"          { key: 'scope', value: scope }",
					"        ]",
					"      }",
					"    }, (err, res) => {",
					"      if (err) { console.error('[auth] token error', err); return; }",
					"      const j = res.json();",
					"      if (j && j.access_token) {",
					"        pm.collectionVariables.set('access_token', j.access_token);",
					"        pm.collectionVariables.set('access_token_ts', String(Date.now()));",
					"        console.log('[auth] token saved to collection variable access_token');",
					"      } else {",
					"        console.error('[auth] no access_token in response', res.text());",
					"      }",
					"    });",
					"  }",
					"}",
					"",
					"// Always attach whatever token we currently have",
					"const tok = pm.collectionVariables.get('access_token');",
					"if (tok) pm.request.headers.upsert({ key: 'Authorization', value: 'Bearer ' + tok });"
				],
				"type": "text/javascript",
				"packages": {}
			}
		}

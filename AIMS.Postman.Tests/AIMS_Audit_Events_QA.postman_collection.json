		{
			"name": "2) Events — ETag Revalidate (expect 304)",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"pm.request.headers.upsert({",
							"  key: 'If-None-Match',",
							"  value: pm.environment.get('etag') || ''",
							"});"
						],
						"type": "text/javascript",
						"packages": {}
					}
				},
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test('304 (unchanged) or 200 (new data) acceptable', () =>",
							"  pm.expect([200,304]).to.include(pm.response.code));",
							"",
							"if (pm.response.code === 304) {",
							"  pm.test('No body on 304', () =>",
							"    pm.expect(pm.response.text()).to.satisfy(t => t === '' || t === undefined));",
							"",
							"  const nxt = pm.environment.get('nextSince_tmp');",
							"  if (nxt) {",
							"    pm.environment.set('since', nxt);",
							"    pm.environment.unset('nextSince_tmp');",
							"  }",
							"} else {",
							"  const etag = pm.response.headers.get('ETag');",
							"  if (etag) pm.environment.set('etag', etag);",
							"",
							"  try {",
							"    const body = pm.response.json();",
							"    if (body && body.nextSince) {",
							"      pm.environment.set('since', body.nextSince);",
							"      pm.environment.unset('nextSince_tmp');",
							"    }",
							"  } catch (e) {",
							"    console.warn('[step2] 200 but body parse failed', e);",
							"  }",
							"}"
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
